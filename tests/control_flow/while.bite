native print;

let a = 0;
while a < 10 {
    a += 1;
}
print(a);

let b = 0;
while b < 10 {
    b += 1;
    if b % 2 == 0 { continue; }
    print(b);
}

let c = 0;
let d = while c < 10 {
    c += 1;
    if c == 5 { break 3 * 4 }
};
print(d);



let e = 0;
@outer: while true {
    e += 1;
    let f = 0;
    @middle: while f < 10 {
        f += 1;
        let g = 0;
        @inner: while g < 10 {
            g += 1;
            if g % 2 == 0 { continue }
            if e + f + g == 25 {
                print(e);
                print(f);
                print(g);
                break@outer
            }
        }
    }
}
print(e);

let h = 0;
let i = @outer: while true {
    while true {
        h += 1;
        if h == 5 { break@outer 10 }
    }
};
print(h);
print(i);

let j = 0;
let k = while j < 10 {
    j += 1;
};
print(k);