class Foo {
    x;
    init(x) {
        this.x = x;
    }
}

class Bar {
    y;
    init(x, y) : super(x) {
        this.y = y;
    }
}

let instance = Bar(5, 10);
print(instance.x);
print(instance.y);

let instance2 = Bar(3, 4);
print(instance2.x);
print(instance2.y);

print(instance.x);
print(instance.y);