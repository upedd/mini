native print;

class Foo {
    get x {
        print("foo.x");
        return 10;
    }

    set x(value) {
        print("=foo.x");
    }
}

class Bar : Foo {
    override get x {
        print("bar.x");
        return 20;
    }

    override set x(value) {
        print("=bar.x");
    }

    get_super_x() {
        return super.x;
    }

    #set_super_x() {
    #    super.x = 10;
    #}
}

let instance = Bar();

instance.x = 10;
print(instance.x);

#instance.set_super_x();
print(instance.get_super_x());

instance.x = 10;
print(instance.x);