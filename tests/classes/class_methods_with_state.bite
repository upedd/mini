class Foo {
    bar;
    baz = 10;

    set_bar(bar) {
        this.bar = bar;
    }

    add() {
        return this.bar + this.baz;
    }
}
native print;

let instance = Foo();
print(instance.bar);
print(instance.baz);

instance.set_bar(8);
print(instance.bar);
print(instance.baz);
print(instance.add());

instance.set_bar(-3);
print(instance.bar);
print(instance.baz);
print(instance.add());

let instance2 = Foo();
print(instance2.bar);
print(instance2.baz);

instance2.set_bar(5);
print(instance2.bar);
print(instance2.baz);
print(instance2.add());

print(instance.bar);
print(instance.baz);
print(instance.add());